<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chat_navigation"
    app:startDestination="@id/chatFragment">

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.fragmentshomework.fragments.ChatFragment"
        android:label="ChatFragment"
        tools:layout="@layout/main_layout">
        <argument
            android:name="count"
            android:defaultValue="0"
            app:argType="integer" />
        <action
            android:id="@+id/action_chatFragment_to_dictionaryFragment"
            app:destination="@id/dictionaryFragment" />
        <action
            android:id="@+id/action_chatFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_chatFragment_self2"
            app:destination="@id/chatFragment" />
    </fragment>


</navigation>